
        class Solution {
  public int search(int[] nums, int target) {
    int mid ,si=0,ei=nums.length-1;
      while (si<=ei){
          mid=si+(ei-si)/2;
          if(nums[mid]==target) return mid;
          if(nums[mid]<target) si=mid+1;
          else ei=mid -1;
      }
      return -1;
  }
}
