import java.util.*;
public class Main
{
	public static void main(String[] args) {
		// find the pair of elements whoes sum is equal to num
		Scanner sc=new Scanner (System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for (int i=0;i<n;i++){
		    arr[i]=sc.nextInt();
		}
		int num=sc.nextInt();
		for (int i=0;i<n;i++){
		    for (int j=i;j<n;j++){
		        if (arr[i]+arr[j]==num&&i!=j){
		            System.out.println(i+" "+j);
		        }
		    }
		
		}
	    
	}
}
